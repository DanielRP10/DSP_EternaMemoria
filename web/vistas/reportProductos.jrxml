	<queryString language="SQL">
		<![CDATA[SELECT 
    p.idProducto,
    p.nombre AS Producto,
    COUNT(dv.idDetalleVentas) AS VecesVendido,
    YEAR(v.fecha) AS Año
FROM 
    detalleVentas dv
INNER JOIN 
    productos p ON dv.idProducto = p.idProducto
INNER JOIN 
    ventas v ON dv.idVenta = v.idVenta
WHERE 
    v.fecha >= DATEADD(YEAR, -1, GETDATE())
GROUP BY 
    p.idProducto, p.nombre, YEAR(v.fecha)
ORDER BY 
    Año DESC, VecesVendido DESC;]]>
	</queryString>
