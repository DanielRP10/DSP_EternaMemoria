	<queryString language="SQL">
		<![CDATA[SELECT 
    vt.idVenta,
    vt.fecha,
    v.idVendedor,
    v.nombre AS vendedorNombre,
    c.idCliente,
    cl.nombre AS clienteNombre,
    YEAR(vt.fecha) AS anio
FROM 
    ventas vt
INNER JOIN 
    contratos c ON vt.idContrato = c.idContrato  -- Relaciona ventas con contratos
INNER JOIN 
    vendedores v ON c.idVendedor = v.idVendedor  -- Relaciona contratos con vendedores
INNER JOIN 
    clientes cl ON c.idCliente = cl.idCliente  -- Relaciona contratos con clientes
ORDER BY 
    v.idVendedor, vt.fecha;]]>
	</queryString>
