	<queryString language="SQL">
		<![CDATA[SELECT 
    ctr.idContrato,
    YEAR(ctr.fechaContrato) AS anio,
    v.nombre AS vendedorNombre,
    (SELECT COUNT(*) 
     FROM contratos ctr2 
     WHERE ctr2.idVendedor = v.idVendedor 
           AND YEAR(ctr2.fechaContrato) = YEAR(ctr.fechaContrato)
    ) AS totalContratos
FROM 
    contratos ctr
INNER JOIN 
    vendedores v ON ctr.idVendedor = v.idVendedor
ORDER BY 
    v.nombre, anio, ctr.idContrato;]]>
	</queryString>
